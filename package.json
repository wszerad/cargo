{
	"name": "cargo",
	"version": "0.1.0",
	"description": "Vue3 inspired simple store, compatible with vue-devtools",
	"author": "wszerad91@gmail.com",
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"example": "parcel serve example/index.html -d dist --public-url /dist-example/ --open"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"dependencies": {
		"@vue/reactivity": "^3.0.0-alpha.4",
		"@vue/runtime-core": "^3.0.0-alpha.4",
		"typescript": "^3.7.4"
	},
	"peerDependencies": {
		"@vue/reactivity": "^3.0.0-alpha.4",
		"@vue/runtime-core": "^3.0.0-alpha.4"
	},
	"devDependencies": {
		"@types/jest": "25.1.1",
		"@types/node": "13.5.3",
		"jest": "25.1.0",
		"ts-jest": "25.1.0",
		"tslint": "6.0.0",
		"parcel-bundler": "^1.12.4",
		"vue": "^3.0.0-alpha.4",
		"oldvue": "npm:vue@2.6.4"
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"isolatedModules": false,
				"diagnostics": true
			}
		},
		"preset": "ts-jest",
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
